// A launch configuration that compiles the extension and then opens it inside a new window
{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "üç∫test",
			"type": "lua",
			"request": "launch",
			"stopOnEntry": false,
			"program": "${workspaceRoot}/test.lua",
			"luaexe": "${workspaceFolder}/bin/lua-language-server.exe",
			"cpath": null,
			"arg": [
			],
			"luaVersion": "5.4",
			"consoleCoding": "utf8",
			"sourceCoding": "utf8",
			"console": "integratedTerminal",
			"outputCapture": [
				"print",
				"stderr",
			],
		},
		{
			"name": "üçÑattach",
			"type": "lua",
			"request": "attach",
			"stopOnEntry": false,
			"address": "127.0.0.1:11413",
			"outputCapture": [
			],
			"sourceMaps": [
				[
					"./*",
					"${workspaceFolder}/*"
				]
			]
		},
		{
			"name": "üçâbuild-3rd-meta",
			"type": "lua",
			"request": "launch",
			"stopOnEntry": false,
			"luaexe": "${workspaceFolder}/bin/lua-language-server",
			"program": "${workspaceRoot}/tools/build-3rd-meta.lua",
			"cpath": "${workspaceFolder}/bin/?.dll",
			"arg": [
			],
			"luaVersion": "latest",
			"consoleCoding": "utf8",
			"sourceCoding": "utf8",
			"outputCapture": [
				"print",
				"stderr",
			],
		},
		{
			"name": "üçñcli-check",
			"type": "lua",
			"request": "launch",
			"stopOnEntry": false,
			"program": "${workspaceRoot}/main.lua",
			"luaexe": "${workspaceFolder}/bin/lua-language-server.exe",
			"cpath": null,
			"arg": [
				"--check",
				"${workspaceRoot}",
			],
			"luaVersion": "5.4",
			"consoleCoding": "utf8",
			"sourceCoding": "utf8",
			"outputCapture": [
				"print",
				"stderr",
			],
		},
	]
}
